                </div>
              </div>
              <div class="c-cal__container c-calendar__style">
                <script>
                
                // CAHNGE the below variable to the CURRENT YEAR
                year = 2022;
          
                // first day of the week of the new year
                today = new Date("1월 1, " + year);
                start_day = today.getDay() + 1;
                fill_table("1월", 31, "01");
                fill_table("2월", 28, "02");
                fill_table("3월", 31, "03");
                fill_table("4월", 30, "04");
                fill_table("5월", 31, "05");
                fill_table("6월", 30, "06");
                fill_table("7월", 31, "07");
                fill_table("8월", 31, "08");
                fill_table("9월", 30, "09");
                fill_table("10월", 31, "10");
                fill_table("11월", 30, "11");
                fill_table("12월", 31, "12");
                </script>
              </div>
