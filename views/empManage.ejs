           <div class="content">
    
            <div class="container">
              <h2 class="mb-5" style="margin-left: 461px;">전체 근무자 관리</h2>
        
              <div class="col-sm-6">
                <div class="search-box">
                    <div class="input-group" style="bottom: 0px;left: 300px;">								
                        <input type="text" id="search" class="form-control" placeholder="팀별 검색">
                        <span class="input-group-addon"><i class="material-icons">&#xE8B6;</i></span>
                    </div>
                </div>
              </div>
              <br>
              <br>
              <div class="table-responsive">

<div class="col-sm-4">
  <button type="button" class="btn btn-info add-new" style="width: 86px;height: 34px;" > 신규+ </button>
</div>
<br>
                <table class="table custom-table">
                  <thead>
                    <tr>
                      <th scope="col">
                        <label class="control control--checkbox">
                          <input type="checkbox" class="js-check-all"/>
                          <div class="control__indicator"></div>
                        </label>
                      </th>
                      <th scope="col">사번</th>
                      <th scope="col">이름</th>
                      <th scope="col">부서명</th>
                      <th scope="col">팀명</th>
                      <th scope="col">직급</th>
                      <th scope="col">입사일</th>
                      <th scope="col">연락처</th>
                      <th scope="col">편집</th>
                    </tr>
                  </thead>
                  <tbody>

                    <% for(let i=0; i<emps.length; i++){%>
                    <tr>
                      <th scope="row">
                        <label class="control control--checkbox">
                          <input type="checkbox"/>
                          <div class="control__indicator"></div>
                        </label>
                      </th>
                      <td style="width: 124px;">
                        <%=emps[i].empNo%>
                      </td>
                      
                      <td style="width: 150px;">
                        <%=emps[i].empName%>
                      </td>
                      
                      <!-- <td style="width: 200px;">
                        <%=emps[i].deptName%>
                        <small class="d-block"><%=emps[i].teamName%></small>
                      </td> -->

                      <td style="width: 190px;">
                        <%=emps[i].deptName%>
                    </td>

                    <td style="width: 140px;">
                      <%=emps[i].teamName%>
                  </td>
                      
                      <td style="width: 140px;">
                          <%=emps[i].position%>
                      </td>
                      
                      <td style="width: 200px;"><%=emps[i].empEntry%>
                      </td>
                      
                      <td style="width: 200px;">
                        <%=emps[i].phoneNum%>
                        </td>
                  
                        <td>
                          <a class="add" title="저장" data-toggle="tooltip"><i class="material-icons">&#xE03B;</i></a>
                          <a class="edit" title="수정" data-toggle="tooltip"><i class="material-icons">&#xE254;</i></a>
                          <a class="delete" title="삭제" data-toggle="tooltip"><i class="material-icons">&#xE872;</i></a>
                      </td>

                    </tr>

                    <%}%>
                    
                  </tbody>
                </table>
              </div>
        
        
            </div>
        
